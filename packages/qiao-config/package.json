{
  "name": "qiao-config",
  "version": "4.7.4",
  "description": "json config to local file",
  "keywords": [
    "json",
    "config",
    "local",
    "file",
    "database"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-config",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src",
    "config.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "qiao-file": "^4.7.4"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
