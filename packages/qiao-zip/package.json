{
  "name": "qiao-zip",
  "version": "4.7.4",
  "description": "nodejs zip and unzip tool",
  "keywords": [
    "zip",
    "unzip",
    "adm-zip",
    "archiver"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-zip",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "archiver": "^7.0.0",
    "iconv-lite": "^0.6.3",
    "qiao-file": "^4.7.4"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
