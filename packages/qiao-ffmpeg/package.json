{
  "name": "qiao-ffmpeg",
  "version": "4.8.3",
  "description": "nodejs ffmpeg tool",
  "keywords": [
    "ffmpeg"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-ffmpeg",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.3",
    "qiao.log.js": "^3.7.3"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
