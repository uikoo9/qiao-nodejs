{
  "name": "qiao-img-cli",
  "version": "5.0.6",
  "description": "nodejs img cli tool",
  "keywords": [
    "nodejs",
    "img convert",
    "img resize",
    "img meta",
    "jpg",
    "png",
    "webp"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-img-cli",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "bin",
    "src"
  ],
  "bin": {
    "qimg": "./bin/qimg.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js"
  },
  "dependencies": {
    "qiao-cli": "^5.0.0",
    "qiao-img": "^5.0.6"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
