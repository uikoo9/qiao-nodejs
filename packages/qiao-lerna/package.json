{
  "name": "qiao-lerna",
  "version": "4.0.0",
  "description": "lerna tools",
  "keywords": [
    "lerna",
    "tools"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-lerna",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "bin",
    "src"
  ],
  "bin": {
    "qlerna": "./bin/qlerna.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "npm-check-updates": "^16.14.5",
    "qiao-cli": "^4.0.0",
    "qiao-console": "^4.0.0",
    "qiao-file": "^4.0.0",
    "qiao-npms": "^4.0.0",
    "qiao-parallel": "^4.0.0"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
