{
  "name": "qiao-file",
  "version": "5.0.1",
  "description": "nodejs file tool",
  "keywords": [
    "fs",
    "tool"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-file",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "debug": "^4.4.3",
    "fs-extra": "^11.3.3",
    "qiao-encode": "^5.0.1"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
